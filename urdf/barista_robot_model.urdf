<?xml version="1.0"?>
<robot name="barista_bot">

    <material name="white">
        <color rgba="1 1 1 1" />
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <!-- BASE FOOTPRINT-->

    <link name="base_footprint">
    </link>


    <!-- BASE LINK -->
    <joint name="base_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.1127"/>
    </joint>

    <link name="base_link">
        <inertial>
            <mass value="10.0" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00047406249999999994" ixy="0.0" ixz="0.0" iyy="0.00047406249999999994" iyz="0.0" izz="0.000640625" />
        </inertial>
        
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.178" length="0.155"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.178" length="0.155"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="base_link">
        <material>Gazebo/Blue</material>
    </gazebo>


    
    <!-- Cup_Holder_Tray_link -->
    <joint name="Cup_Holder_Tray_Joint" type="fixed">
        <parent link="base_link"/>
        <child link="Cup_Holder_Tray_link"/>
        <origin xyz="0 0 0.3425"/>
    </joint>

    <link name="Cup_Holder_Tray_link">
        <inertial>
            <mass value="10.0" />
            <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="0.00047406249999999994" ixy="0.0" ixz="0.0" iyy="0.00047406249999999994" iyz="0.0" izz="0.000640625" />
        </inertial>
        
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.160" length="0.090"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.160" length="0.090"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="Cup_Holder_Tray_link">
        <material>Gazebo/Blue</material>
    </gazebo>


    <!-- Standoff_Rod_1_link -->
    <joint name="Standoff_Rod_1_Joint" type="fixed">
        <parent link="base_link"/>
        <child link="Standoff_Rod_1_link"/>
        <origin xyz="0.1 0.1 0.1875"/>
    </joint>

    <link name="Standoff_Rod_1_link">
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000047406249999999994" ixy="0.0" ixz="0.0" iyy="0.000047406249999999994" iyz="0.0" izz="0.0000640625" />
        </inertial>
        
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="Standoff_Rod_1_link">
        <material>Gazebo/white</material>
    </gazebo>

    <!-- Standoff_Rod_2_link -->
    <joint name="Standoff_Rod_2_Joint" type="fixed">
        <parent link="base_link"/>
        <child link="Standoff_Rod_2_link"/>
        <origin xyz="-0.1 -0.1 0.1875"/>
    </joint>

    <link name="Standoff_Rod_2_link">
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000047406249999999994" ixy="0.0" ixz="0.0" iyy="0.000047406249999999994" iyz="0.0" izz="0.0000640625" />
        </inertial>
        
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="Standoff_Rod_2_link">
        <material>Gazebo/white</material>
    </gazebo>

    <!-- Standoff_Rod_3_link -->
    <joint name="Standoff_Rod_3_Joint" type="fixed">
        <parent link="base_link"/>
        <child link="Standoff_Rod_3_link"/>
        <origin xyz="0.1 -0.1 0.1875"/>
    </joint>

    <link name="Standoff_Rod_3_link">
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000047406249999999994" ixy="0.0" ixz="0.0" iyy="0.000047406249999999994" iyz="0.0" izz="0.0000640625" />
        </inertial>
        
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="Standoff_Rod_3_link">
        <material>Gazebo/white</material>
    </gazebo>

    <!-- Standoff_Rod_4_link -->
    <joint name="Standoff_Rod_4_Joint" type="fixed">
        <parent link="base_link"/>
        <child link="Standoff_Rod_4_link"/>
        <origin xyz="-0.1 0.1 0.1875"/>
    </joint>

    <link name="Standoff_Rod_4_link">
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000047406249999999994" ixy="0.0" ixz="0.0" iyy="0.000047406249999999994" iyz="0.0" izz="0.0000640625" />
        </inertial>
        
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.22"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="Standoff_Rod_4_link">
        <material>Gazebo/white</material>
    </gazebo>




    <!-- LEFT WHEEL LINK -->

    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.15 -0.0775" rpy="-1.57 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

<link name="left_wheel">
    <inertial>
        <mass value="1.5" />
        <origin xyz="0 0 0" rpy="-1.57 0 0" />
        <inertia ixx="0.000346875" ixy="0.0" ixz="0.0" iyy="0.000346875" iyz="0.0" izz="0.00046875" />
    </inertial>
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <cylinder radius="0.0352" length="0.0206"/>
        </geometry>
        <material name="black"/>
    </visual>
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <cylinder radius="0.0352" length="0.0206"/>
        </geometry>
    </collision>
</link>

    <gazebo reference="left_wheel">
        <mu1 value="5.0"/>
		<mu2 value="0.5"/>
		<kp value="10000000.0" />
		<kd value="1.0" />
		<fdir1 value="1 0 0"/>
        <material>Gazebo/Black</material>
    </gazebo>


    <!-- RIGHT WHEEL LINK -->

    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.15 -0.0775" rpy="1.57 0 0" />
        <axis xyz="0 0 -1"/>
    </joint>

    <link name="right_wheel">
        <inertial>
            <mass value="1.5" />
            <origin xyz="0 0 0" rpy="-1.575 0 0" />
            <inertia ixx="0.000346875" ixy="0.0" ixz="0.0" iyy="0.000346875" iyz="0.0" izz="0.00046875" />
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.0352" length="0.0206"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.0352" length="0.0206"/>
            </geometry>
        </collision>
    </link>
    <gazebo reference="right_wheel">
        <mu1 value="5.0"/>
		<mu2 value="0.5"/>
		<kp value="10000000.0" />
		<kd value="1.0" />
		<fdir1 value="1 0 0"/>
        <material>Gazebo/Black</material>
    </gazebo>

    <!-- FRONT CASTER WHEEL -->

    <joint name="front_caster_wheel_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_caster"/>
        <origin xyz="0.1 0.0 -0.0775" rpy="0 0 0" />
        <axis xyz="0 0 0"/>
    </joint>

    <link name="front_caster">
        <visual>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00012500000000000003" ixy="0.0" ixz="0.0" iyy="0.00012500000000000003" iyz="0.0" izz="0.00012500000000000003" />
        </inertial>
    </link>

    <gazebo reference="front_caster">
        <mu1 value="0.0"/>
        <mu2 value="0.0"/>
        <kp value="10000000.0" />
        <kd value="100.0" />
        <fdir1 value="0 0 1"/>
        <material>Gazebo/Gray</material>
    </gazebo>


    <!-- REAR CASTER WHEEL -->

    <joint name="rear_caster_wheel_joint" type="fixed">
        <parent link="base_link"/>
        <child link="back_caster"/>
        <origin xyz="-0.1 0.0 -0.0775" rpy="0 0 0" />
        <axis xyz="0 0 0"/>
    </joint>

    <link name="back_caster">
        <visual>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00012500000000000003" ixy="0.0" ixz="0.0" iyy="0.00012500000000000003" iyz="0.0" izz="0.00012500000000000003" />
        </inertial>
    </link>

    <gazebo reference="back_caster">
        <mu1 value="0.0"/>
        <mu2 value="0.0"/>
        <kp value="10000000.0" />
        <kd value="100.0" />
        <fdir1 value="0 0 1"/>
        <material>Gazebo/Gray</material>
    </gazebo>
 
   <!-- Gazebo tags - Diff driver -->
      <gazebo>
        <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">

        <!-- wheels -->
        <left_joint>left_wheel_joint</left_joint>
        <right_joint>right_wheel_joint</right_joint>

        <!-- kinematics -->
        <wheel_separation>0.3</wheel_separation>
        <wheel_diameter>0.0704</wheel_diameter>

        <!-- limits -->
        <max_wheel_torque>1.0</max_wheel_torque>
        <max_wheel_acceleration>2.0</max_wheel_acceleration>

        <!-- output -->
        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>

        <odometry_frame>odom</odometry_frame>
        <robot_base_frame>base_footprint</robot_base_frame>

        </plugin>
    </gazebo>
    

    
    <!-- Gazebo tags - Laser scan -->
    <gazebo reference="front_laser_joint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="front_laser">
        <material>Gazebo/DarkGrey</material>
    </gazebo>

    <!-- Laser scan -->
    <joint name="front_laser_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_laser"/>
        <origin xyz="0.14 0.0 0.0875" rpy="0 0 0" />
        <axis xyz="0 0 0"/>
    </joint>

    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.05" />
            <inertia ixx="0.000252666666667" ixy="0" ixz="0" iyy="0.000252666666667" iyz="0" izz="0.0005"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <!-- <mesh filename="package://barista_robot_description/meshes/lidar_m10_p.STL" scale="1 1 1"/> -->
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
            <material name="red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="front_laser">
        <sensor name="sensor_ray" type="ray">
            <pose>0 0 0 0 0 0</pose>
            <ray>
            <scan>
                <horizontal>
                <samples>200</samples>
                <resolution>1.0</resolution>
                <min_angle>-1.57</min_angle>
                <max_angle>1.57</max_angle>
                </horizontal>
            </scan>
            <range>
                <min>0.1</min>
                <max>5.0</max>
            </range>
            </ray>
            <always_on>true</always_on>
            <visualize>true</visualize>
            <update_rate>100.0</update_rate>
            <plugin name="laser" filename="libgazebo_ros_ray_sensor.so">
            <ros>
                <!-- <namespace>/box_bot</namespace> -->
                <remapping>~/out:=scan</remapping>
            </ros>
            <output_type>sensor_msgs/LaserScan</output_type>
            </plugin>
        </sensor>
    </gazebo>
</robot>


